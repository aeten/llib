if DEBUG then
  defines = 'LLIB_DEBUG'
end
if WINDOWS and CC=='gcc' then
  defines = (defines or '')..' LLIB_PTR_LIST'
end
c99.library{'llib',
    src='obj sort pool interface list file filew scan map str value template arg json json-data json-parse seq smap xml table farr config',
    defines=defines
}
