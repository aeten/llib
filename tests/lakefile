libs = choose(MSVC,'llib_static','llib')
local function test(f)
    return c99.program{f,incdir='..',libdir='../llib',libs=libs}:run()
end
if P then
  default{ test(P) }
  return
end
default {
    test('test-obj'),
    test('test-list'),
    test('test-file'),
    test('test-scan'),
    test('test-seq'),
    test('test-map'),
    test('test-str'),
    test('test-template'),
    test('test-json'),
    test('test-xml'),
}

